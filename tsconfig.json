{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "plugins": [
      {
        "name": "tslint-language-service",
        "alwaysShowRuleFailuresAsWarnings": true
      }
    ]
  },
  "include": ["./src/*.ts"]
}
